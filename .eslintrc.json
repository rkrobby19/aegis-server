{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "plugin:react/recommended",
    "prettier",
    "airbnb"
  ],
  "overrides": [],
  "settings": {
    "react": {
      "version": "999.999.999"
    }
  },
  "plugins": [
    "react",
    "jest"
  ],
  "rules": {
    "no-console": 0,
    "import/no-import-module-exports": 0,
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "no-empty": 0,
    "import/named": 0,
    "func-names": 0,
    "camelcase": 0
  }
}